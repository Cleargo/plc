<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="editor"/>
    <body>
        <referenceContainer name="content">
            <block class="Cleargo\FindYourLock\Block\Adminhtml\Lock\Edit" name="lock_lock_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Cleargo\FindYourLock\Block\Adminhtml\Lock\Edit\Tabs" name="cleargo_findYourLock_edit_tabs">
                <block class="Cleargo\FindYourLock\Block\Adminhtml\Lock\Edit\Tab\Main213" name="cleargo_findYourLock_edit_tab_main"/>
                <block class="Cleargo\FindYourLock\Block\Adminhtml\Lock\Edit\Tab\Content" name="cleargo_findYourLock_edit_tab_content"/>
                <action method="addTab">
                    <argument name="name" xsi:type="string">main_section</argument>
                    <argument name="block" xsi:type="string">cleargo_findYourLock_edit_tab_main</argument>
                </action>
                <action method="addTab">
                    <argument name="name" xsi:type="string">content_section</argument>
                    <argument name="block" xsi:type="string">cleargo_findYourLock_edit_tab_content</argument>
                </action>
            </block>
        </referenceContainer>

    </body>
</page>
