<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Cleargo_Core::extension">
                    <resource id="Cleargo_FindYourLock::find_your_lock">
                        <resource id="Cleargo_FindYourLock::country" title="Regions" sortOrder="10">
                            <resource id="Cleargo_FindYourLock::country_save" title="Save Region" sortOrder="10" />
                            <resource id="Cleargo_FindYourLock::country_delete" title="Delete Region" sortOrder="20" />
                        </resource>
                        <resource id="Cleargo_FindYourLock::district" title="Districts" sortOrder="20">
                            <resource id="Cleargo_FindYourLock::district_save" title="Save District" sortOrder="10" />
                            <resource id="Cleargo_FindYourLock::district_delete" title="Delete District" sortOrder="20" />
                        </resource>
                        <resource id="Cleargo_FindYourLock::lock" title="Locks" sortOrder="30">
                            <resource id="Cleargo_FindYourLock::lock_save" title="Save Lock" sortOrder="10" />
                            <resource id="Cleargo_FindYourLock::lock_delete" title="Delete Lock" sortOrder="20" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
