<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <referenceBlock name="page.main.title">
        <action method="setPageTitle">
            <argument translate="true" name="title" xsi:type="string">My Warranty Detail</argument>
        </action>
    </referenceBlock>
    <head>
        <css src="Cleargo_Warranty::resources/form.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Cleargo\Warranty\Block\View" name="warranty.warranty.view" cacheable="false">
                <container name="warranty.warranty.history.info" as="info" label="Order History Info"/>
            </block>
        </referenceContainer>
    </body>
</page>
