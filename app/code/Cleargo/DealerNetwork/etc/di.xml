<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Cleargo\DealerNetwork\Api\Data\CountrySearchResultsInterface"
                type="Magento\Framework\Api\SearchResults" />
    <preference for="Cleargo\DealerNetwork\Api\Data\CountryInterface" type="Cleargo\DealerNetwork\Model\Country" />
    <preference for="Cleargo\DealerNetwork\Api\CountryRepositoryInterface" type="Cleargo\DealerNetwork\Model\CountryRepository" />
    <preference for="Cleargo\DealerNetwork\Api\Data\RegionSearchResultsInterface"
                type="Magento\Framework\Api\SearchResults" />
    <preference for="Cleargo\DealerNetwork\Api\Data\RegionInterface" type="Cleargo\DealerNetwork\Model\Region" />
    <preference for="Cleargo\DealerNetwork\Api\RegionRepositoryInterface" type="Cleargo\DealerNetwork\Model\RegionRepository" />
    <preference for="Cleargo\DealerNetwork\Api\Data\DealerSearchResultsInterface"
                type="Magento\Framework\Api\SearchResults" />
    <preference for="Cleargo\DealerNetwork\Api\Data\DealerInterface" type="Cleargo\DealerNetwork\Model\Dealer" />
    <preference for="Cleargo\DealerNetwork\Api\DealerRepositoryInterface" type="Cleargo\DealerNetwork\Model\DealerRepository" />
    <preference for="Cleargo\DealerNetwork\Api\Data\BrandSearchResultsInterface"
                type="Magento\Framework\Api\SearchResults" />
    <preference for="Cleargo\DealerNetwork\Api\Data\BrandInterface" type="Cleargo\DealerNetwork\Model\Brand" />
    <preference for="Cleargo\DealerNetwork\Api\BrandRepositoryInterface" type="Cleargo\DealerNetwork\Model\BrandRepository" />
    <type name="Magento\Framework\View\Element\UiComponent\DataProvider\CollectionFactory">
        <arguments>
            <argument name="collections" xsi:type="array">
                <item name="dealer_country_listing_data_source" xsi:type="string">Cleargo\DealerNetwork\Model\ResourceModel\Country\Grid\Collection</item>
                <item name="dealer_region_listing_data_source" xsi:type="string">Cleargo\DealerNetwork\Model\ResourceModel\Region\Grid\Collection</item>
                <item name="dealer_dealer_listing_data_source" xsi:type="string">Cleargo\DealerNetwork\Model\ResourceModel\Dealer\Grid\Collection</item>
                <item name="dealer_brand_listing_data_source" xsi:type="string">Cleargo\DealerNetwork\Model\ResourceModel\Brand\Grid\Collection</item>
            </argument>
        </arguments>
    </type>
    <type name="Cleargo\DealerNetwork\Model\ResourceModel\Country\Grid\Collection">
        <arguments>
            <argument name="mainTable" xsi:type="string">dealer_country</argument>
            <argument name="eventPrefix" xsi:type="string">dealer_country_grid_collection</argument>
            <argument name="eventObject" xsi:type="string">country_grid_collection</argument>
            <argument name="resourceModel" xsi:type="string">Cleargo\DealerNetwork\Model\ResourceModel\Country</argument>
        </arguments>
    </type>
    <type name="Cleargo\DealerNetwork\Model\ResourceModel\Region\Grid\Collection">
        <arguments>
            <argument name="mainTable" xsi:type="string">dealer_region</argument>
            <argument name="eventPrefix" xsi:type="string">dealer_region_grid_collection</argument>
            <argument name="eventObject" xsi:type="string">region_grid_collection</argument>
            <argument name="resourceModel" xsi:type="string">Cleargo\DealerNetwork\Model\ResourceModel\Region</argument>
        </arguments>
    </type>
    <type name="Cleargo\DealerNetwork\Model\ResourceModel\Dealer\Grid\Collection">
        <arguments>
            <argument name="mainTable" xsi:type="string">dealer_dealer</argument>
            <argument name="eventPrefix" xsi:type="string">dealer_dealer_grid_collection</argument>
            <argument name="eventObject" xsi:type="string">dealer_grid_collection</argument>
            <argument name="resourceModel" xsi:type="string">Cleargo\DealerNetwork\Model\ResourceModel\Dealer</argument>
        </arguments>
    </type>
    <type name="Cleargo\DealerNetwork\Model\ResourceModel\Brand\Grid\Collection">
        <arguments>
            <argument name="mainTable" xsi:type="string">dealer_brand</argument>
            <argument name="eventPrefix" xsi:type="string">dealer_brand_grid_collection</argument>
            <argument name="eventObject" xsi:type="string">brand_grid_collection</argument>
            <argument name="resourceModel" xsi:type="string">Cleargo\DealerNetwork\Model\ResourceModel\Brand</argument>
        </arguments>
    </type>
    <virtualType name="DealerGirdFilterPool" type="Magento\Framework\View\Element\UiComponent\DataProvider\FilterPool">
        <arguments>
            <argument name="appliers" xsi:type="array">
                <item name="regular" xsi:type="object">Magento\Framework\View\Element\UiComponent\DataProvider\RegularFilter</item>
                <item name="fulltext" xsi:type="object">Magento\Framework\View\Element\UiComponent\DataProvider\FulltextFilter</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="CountryGridDataProvider" type="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
        <arguments>
            <argument name="collection" xsi:type="object" shared="false">Cleargo\DealerNetwork\Model\ResourceModel\Country\Collection</argument>
            <argument name="filterPool" xsi:type="object" shared="false">DealerGirdFilterPool</argument>
        </arguments>
    </virtualType>
    <virtualType name="RegionGridDataProvider" type="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
        <arguments>
            <argument name="collection" xsi:type="object" shared="false">Cleargo\DealerNetwork\Model\ResourceModel\Region\Collection</argument>
            <argument name="filterPool" xsi:type="object" shared="false">DealerGirdFilterPool</argument>
        </arguments>
    </virtualType>
    <virtualType name="DealerGridDataProvider" type="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
        <arguments>
            <argument name="collection" xsi:type="object" shared="false">Cleargo\DealerNetwork\Model\ResourceModel\Dealer\Collection</argument>
            <argument name="filterPool" xsi:type="object" shared="false">DealerGirdFilterPool</argument>
        </arguments>
    </virtualType>
    <virtualType name="BrandGridDataProvider" type="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
        <arguments>
            <argument name="collection" xsi:type="object" shared="false">Cleargo\DealerNetwork\Model\ResourceModel\Brand\Collection</argument>
            <argument name="filterPool" xsi:type="object" shared="false">DealerGirdFilterPool</argument>
        </arguments>
    </virtualType>
</config>
